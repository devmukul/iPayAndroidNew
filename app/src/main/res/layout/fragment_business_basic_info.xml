<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingTop="@dimen/activity_vertical_margin">

    <bd.com.ipay.ipayskeleton.CustomView.IconifiedTextViewWithButton
        android:id="@+id/button_business_information"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_bizinfo"
        android:paddingBottom="@dimen/value4"
        android:paddingTop="@dimen/value4"
        android:text="@string/business_information" />

    <View
        android:layout_width="wrap_content"
        android:layout_height="@dimen/divider_space"
        android:background="@color/colorDivider" />


    <bd.com.ipay.ipayskeleton.CustomView.IconifiedTextViewWithButton
        android:id="@+id/business_contact"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_bizinfo"
        android:paddingBottom="@dimen/value4"
        android:paddingTop="@dimen/value4"
        android:text="@string/business_contact" />


</LinearLayout>

