<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingTop="@dimen/activity_vertical_margin">

    <bd.com.ipay.ipayskeleton.CustomView.IconifiedTextViewWithButton
        android:id="@+id/button_business_information"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_bizinfo"
        android:paddingBottom="@dimen/value4"
        android:paddingTop="@dimen/value4"
        android:text="@string/business_information" />

    <View style="@style/DividerWithoutMargin" />

    <bd.com.ipay.ipayskeleton.CustomView.IconifiedTextViewWithButton
        android:id="@+id/button_manage_employees"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_manageemployees"
        android:paddingBottom="@dimen/value4"
        android:paddingTop="@dimen/value4"
        android:text="@string/manage_employees" />

    <View style="@style/DividerWithoutMargin" />
</LinearLayout>